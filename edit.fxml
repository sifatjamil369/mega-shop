<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EditController">
    <children>
        <TextField fx:id="pricei" layoutX="226.0" layoutY="249.0" promptText="Price" />
        <TextField fx:id="brandi" layoutX="226.0" layoutY="167.0" promptText="Brand" />
        <TextField fx:id="namei" layoutX="226.0" layoutY="126.0" promptText="Name" />
        <Text layoutX="221.0" layoutY="84.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Edit Product">
            <font>
                <Font size="30.0" />
            </font>
        </Text>
        <Button fx:id="addbtn" layoutX="227.0" layoutY="313.0" mnemonicParsing="false" onAction="#add" text="Submit" />
        <Button fx:id="cancel" layoutX="322.0" layoutY="313.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
        <ChoiceBox fx:id="categoryBox" layoutX="225.0" layoutY="206.0" prefHeight="28.0" prefWidth="152.0" />
    </children>
</AnchorPane>
